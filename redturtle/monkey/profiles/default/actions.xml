<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="create_campaign" meta_type="CMF Action" i18n:domain="redturtle.monkey">
   <property name="title"
      i18n:translate="">Create Mailchimp campaign</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object/absolute_url}/@@create_campaign</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">string:${object_url}/@@can_create_campaign</property>
   <property name="permissions">
    <element value="redturtle.monkey: Add Campaign"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>

